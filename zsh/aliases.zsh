#Git aliases
alias glol = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)' --all
alias ga = git add
alias gs='nocorrect git status'
alias gc = git commit 
alias gco = git checkout
alias gb = git branch
alias gp = git push
alias gpom = git push origin main
alias gref = git reflog
alias gd = git diff

#Programs
alias vim='nvim'

#Utility
alias dotfiles="cd ~/.dotfiles"