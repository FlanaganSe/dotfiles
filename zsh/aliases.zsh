#Git aliases
alias glol='--pretty=format:"%C(auto)%h %ad | %s%d [%an]%C(reset)" --graph --date=short'
alias ga='git add'
alias gs='nocorrect git status'
alias gc='git commit '
alias gco='git checkout'
alias gb='git branch'
alias gp='git push'
alias gpom='git push origin main'
alias gref='git reflog'
alias gd='git diff'

#Programs
alias vim='nvim'

#Utility
alias dotfiles="cd ~/.dotfiles"